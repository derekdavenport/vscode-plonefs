{
	"name": "vscode-plonefs",
	"displayName": "PloneFS",
	"description": "Open Plone sites",
	"version": "0.1.0",
	"publisher": "Derek Davenport",
	"repository": {
		"url": "https://github.com/derekdavenport/plonefs.git",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:plone",
		"onCommand:plonefs.workspace"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "plonefs.workspace",
				"title": "Open Plone Workspace",
				"category": "PloneFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "plonefs.workspace",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"vscode:package": "vsce package",
		"vscode:publish": "vsce publish",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:integration": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/node": "^10.12.10",
		"@types/jest": "^23.3.9",
		"typescript": "^3.1.6",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0"
	},
	"dependencies": {
		"mime": "^2.3.1"
	}
}
