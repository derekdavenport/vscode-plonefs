{
	"name": "vscode-plonefs",
	"displayName": "Plone File System",
	"description": "Open Plone sites in Visual Studio Code",
	"icon": "images/PloneFS.png",
	"version": "0.5.0",
	"publisher": "derekdavenport",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/derekdavenport/vscode-plonefs",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:plone",
		"onCommand:plonefs.workspace"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "plonefs.workspace",
				"title": "Add Plone site to Workspace",
				"category": "PloneFS"
			},
			{
				"command": "plonefs.optionsMenu",
				"title": "Plone Options",
				"category": "PloneFS"
			},
			{
				"command": "plonefs.changeState",
				"title": "Change State",
				"category": "PloneFS"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "plonefs.optionsMenu",
					"group": "PloneFS",
					"when": "resourceScheme == plone"
				}
			],
			"commandPalette": [
				{
					"command": "plonefs.optionsMenu",
					"when": "false"
				},
				{
					"command": "plonefs.changeState",
					"when": "false"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"vscode:package": "vsce package",
		"vscode:publish": "vsce publish",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch --info-verbosity verbose",
		"webpack": "webpack --mode none",
		"test-compile": "tsc -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:integration": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/form-data": "^2.2.1",
		"@types/got": "^9.4.3",
		"@types/node": "^10.11.0",
		"@types/vscode": "^1.39.0",
		"copyfiles": "^2.1.1",
		"file-loader": "^3.0.1",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.7.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"form-data": "^2.5.1",
		"got": "^9.6.0",
		"mime": "^2.4.4",
		"node-html-parser": "^1.1.16",
		"tough-cookie": "^3.0.1"
	}
}
