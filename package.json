{
	"name": "vscode-plonefs",
	"displayName": "Plone File System",
	"description": "Open Plone sites in Visual Studio Code",
	"icon": "images/PloneFS.png",
	"version": "0.3.3",
	"publisher": "derekdavenport",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/derekdavenport/vscode-plonefs",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:plone",
		"onCommand:plonefs.workspace"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "plonefs.workspace",
				"title": "Add Plone site to Workspace",
				"category": "PloneFS"
			},
			{
				"command": "plonefs.editSettings",
				"title": "Plone Options",
				"category": "PloneFS"
			},
			{
				"command": "plonefs.changeState",
				"title": "Change State",
				"category": "PloneFS"
			},
			{
				"command": "plonefs.debug.expireCookie",
				"title": "Expire Cookie",
				"category": "PloneFS"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "plonefs.editSettings",
					"group": "PloneFS",
					"when": "resourceScheme == plone"
				}
			],
			"commandPalette": [
				{
					"command": "plonefs.editSettings",
					"when": "resourceScheme == plone"
				},
				{
					"command": "plonefs.changeState",
					"when": "resourceScheme == plone"
				},
				{
					"command": "plonefs.debug.expireCookie",
					"when": "inDebugMode"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"vscode:package": "vsce package",
		"vscode:publish": "vsce publish",
		"copyfiles": "npx copyfiles ssl/* out",
		"compile": "npm run copyfiles && tsc -p ./",
		"watch": "npm run copyfiles && tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:integration": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/jest": "^24.0.6",
		"@types/node": "^10.12.26",
		"copyfiles": "^2.1.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.3",
		"vscode": "^1.1.29"
	},
	"dependencies": {
		"mime": "^2.4.0",
		"ssl-root-cas": "^1.3.1"
	}
}
